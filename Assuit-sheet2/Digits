#include <stdio.h>
#include <stdlib.h>

int main()
{
    unsigned char test_cases;
    unsigned char i, j;
    unsigned char a;
    unsigned char digits = 0;

    scanf("%u",&test_cases);

    unsigned long long * ptr = (unsigned long long *)malloc(sizeof(unsigned long long) * test_cases);

    for(i = 0; i < test_cases; i++)
    {
        scanf("%llu",ptr+i);
    }


    for(i = 0; i < test_cases; i++)
    {

       while(ptr[i]>0)
       {
           printf("%llu ",ptr[i]%10);
           ptr[i] = ptr[i]/10;
       }

        printf("\n");
    }


    return 0;
}
